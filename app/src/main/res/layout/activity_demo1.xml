<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:orientation="vertical">

        <Button
            android:id="@+id/open_browser"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="打开浏览器百度首页" />

        <Button
            android:id="@+id/toPrinterActivityWithRequestLogin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到数据打印页(使用动态登录检查)" />

        <Button
            android:id="@+id/toPrinterActivityWithInterceptor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到数据打印页(使用指定登录拦截器)" />

        <Button
            android:id="@+id/toPrinterActivityWithoutInterceptor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到数据打印页(不触发拦截器)" />

        <Button
            android:id="@+id/toUserActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到用户信息页(自带登录拦截器)" />

        <Button
            android:id="@+id/toPrinterActivityWithExtras"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到数据打印页(添加额外数据)" />

        <Button
            android:id="@+id/toResultActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="使用ActivityResult回调" />
    </LinearLayout>


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/fab_margin"
        app:srcCompat="@android:drawable/ic_dialog_email" />

</android.support.design.widget.CoordinatorLayout>
